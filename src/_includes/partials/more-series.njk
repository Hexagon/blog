<!-- More series -->
{% set otherSeries = search.pages("part=1") %}
<nav>
    <h4>Article series</h5>
    <ul>
        {% for subpost in search.pages("part=1") %}
            <li>
                <a class="{% if subpost.data.url == url %}link-secondary{% endif %}" href="{{ subpost.data.url }}">{{ subpost.data.series.title }}</a>
            </li>
        {% endfor %}
    </ul>
</nav>